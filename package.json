{
  "name": "thebest.pictures",
  "version": "1.0.0",
  "description": "Every Oscar Winner for Best Picture. How many of The Best Pictures have you seen?",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "prestart": "npm-run-all clearAssets transpileStartMessage && node dist/tools/startMessage.js",
    "transpileStartMessage": "babel tools/startMessage.js -d dist/tools",
    "transpileBuildHtml": "babel tools/buildHtml.js -d dist/tools",
    "start": "npm run dev",
    "dev": "npm-run-all --parallel serveDev lint:watch",
    "prod": "npm-run-all clearAssets transpileBuildHtml buildHtml serveProd",
    "clearAssets": "babel-node tools/clearAssets.js",
    "buildHtml": "node dist/tools/buildHtml.js",
    "serveDev": "NODE_ENV=development babel-node tools/devServer.js",
    "serveProd": "NODE_ENV=production babel-node tools/prodServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "jasmine"
  },
  "author": "Jon Sanders",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.5",
    "express": "4.16.3",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-rc.1",
    "@babel/core": "^7.0.0-rc.1",
    "@babel/node": "^7.0.0-rc.1",
    "@babel/polyfill": "^7.0.0-rc.1",
    "@babel/preset-env": "^7.0.0-rc.1",
    "@babel/preset-react": "^7.0.0-rc.1",
    "@babel/register": "^7.0.0-rc.1",
    "babel-loader": "^8.0.0-beta.4",
    "babel-preset-react-hmre": "1.1.1",
    "cheerio": "^1.0.0-rc.2",
    "colors": "1.1.2",
    "compression-webpack-plugin": "^2.0.0",
    "css-loader": "0.23.1",
    "enzyme": "^3.4.4",
    "enzyme-adapter-react-16": "^1.2.0",
    "eslint": "^5.4.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "5.0.1",
    "eslint-watch": "4.0.2",
    "file-loader": "^1.1.11",
    "jasmine": "^3.1.0",
    "jasmine-enzyme": "^6.0.0",
    "jsdom": "^12.0.0",
    "less": "3.8.1",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.5",
    "npm-run-all": "1.8.0",
    "opn": "^5.3.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "^1.12.1",
    "proxyquire": "^2.0.1",
    "redux-mock-store": "1.0.2",
    "rimraf": "^2.6.2",
    "style-loader": "0.13.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.1",
    "webpack": "^4.16.5",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jonsanders101/theBest.Pictures"
  }
}